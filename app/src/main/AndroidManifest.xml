<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.leong.leonglearndemo">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name=".LeongLearnApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:resizeableActivity="false"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".eventbus.EventBusActivity1" android:label="@string/event_bus"/>
        <activity android:name=".eventbus.EventBusActivity2" android:label="@string/event_bus"/>
        <activity android:name=".eventbus.StickyEventBusActivity" android:label="@string/event_bus"/>
        <activity android:name=".rxjava.HelloRxjava" android:label="@string/rxjava"/>
        <activity android:name=".retrofit2.RetrofitActivity" android:label="@string/retrofit"/>
        <activity android:name=".oauth_diycode.LoginActivity" android:label="@string/oauth"/>
        <activity android:name=".greendao.GreenDaoActivity" android:label="@string/greendao"/>
        <activity android:name=".okhttp3.OkhttpActivity" android:label="@string/okhttp3"/>
        <activity android:name=".volley.VolleyActivity" android:label="@string/volley"/>
        <activity android:name=".recycleview.LeongRecycleViewActivity" android:label="@string/app_name"/>
    </application>

</manifest>